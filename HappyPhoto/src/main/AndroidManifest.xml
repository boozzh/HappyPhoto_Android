<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.happyz.happyphoto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.happyz.happyphoto.MainActivity"
            android:theme="@style/StartTheme"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserLoginActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserInfoActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserAlbumCreateActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserAlbumListAllActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserAlbumGameActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserAlbumListOfMineActivity"
            android:theme="@style/UserTheme"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.happyz.happyphoto.Gen.User.UserAlbumPicListActivity"
            android:theme="@style/ViewPicStyle"
            android:screenOrientation="portrait" >
        </activity>
    </application>


</manifest>
